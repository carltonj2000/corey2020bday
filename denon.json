{
  "$schema": "https://deno.land/x/denon/schema.json",
  "watcher": {
    "interval": 350,
    "exts": ["js"],
    "match": ["*.*"],
    "skip": [
      "*/.git/*",
      "*/.vscode/*",
      "*/src/*.*",
      "*/node_modules/*",
      "*/build/*",
      "*/resources/*",
      "*/public/*"
    ],
    "legacy": false
  }
}
